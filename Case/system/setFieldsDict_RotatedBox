FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue U -0.0867
    volScalarFieldValue P -1
);

regions
(
   
   rotatedBoxToCell
    {
        origin (2.5e-6 -0.15e-6 0);
        i (0.15e-6 0.15e-6 0);
        j (-0.15e-6 0.15e-6 0);
        k (0 0 1e-6);
        fieldValues
        (
            volScalarFieldValue P 1        
        );
    }
    

);


